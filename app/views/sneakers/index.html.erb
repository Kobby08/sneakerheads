<h1>My Sneakers</h1>
<%= link_to "Home", root_path %>
<%= link_to "Create Sneaker", new_sneaker_path %>
<% @sneakers.each do |sneaker| %>
  <p><%= sneaker.name %></p>
  <p><%= sneaker.model %></p>
  <p><%= sneaker.price %></p>
  <%= image_tag sneaker.image if sneaker.image.present? %>
  <%= link_to "Show Sneaker", sneaker_path(sneaker) %>
  <hr>
<% end %>
